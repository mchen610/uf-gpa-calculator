{
  "manifest_version": 3,
  "name": "one.uf gpa calculator",
  "version": "0.2.0",
  "action": {
    "default_title": "one.uf gpa calculator",
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "one.png",
      "48": "one.png",
      "128": "one.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://one.uf.edu/*"
  ],
  "icons": {
    "16": "one.png",
    "48": "one.png",
    "128": "one.png"
  }
}







